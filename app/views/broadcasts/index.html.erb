<h1>Broadcasts</h1>
<%= link_to 'Channels', channels_path %>
<table>
  <tr>
    <th>Channel</th>
    <th>Broadcaster broadcast</th>
    <th>Start</th>
    <th>End</th>
    <th>Broadcaster program id</th>
    <th>Synopsis</th>
    <th>Title</th>
    <th>Subtitle</th>
    <th>Broadcast tags</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @broadcasts.each do |broadcast| %>
  <tr>
    <td><%= broadcast.channel_id %></td>
    <td><%= broadcast.broadcaster_broadcast_id %></td>
    <td><%= broadcast.start %></td>
    <td><%= broadcast.end %></td>
    <td><%= broadcast.broadcaster_program_id %></td>
    <td><%= broadcast.synopsis %></td>
    <td><%= broadcast.title %></td>
    <td><%= broadcast.subtitle %></td>
    <td><%= broadcast.broadcast_list %></td>
    <td><%= link_to 'Show', broadcast %></td>
    <td><%= link_to 'Edit', edit_broadcast_path(broadcast) %></td>
    <td><%= link_to 'Destroy', broadcast, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Broadcast', new_broadcast_path %>
